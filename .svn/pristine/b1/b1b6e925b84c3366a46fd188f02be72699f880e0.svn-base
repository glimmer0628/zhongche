// vuex/modules/index.js
import {
  SAVE_OPENID
} from '../mutation-types';

// 该模块的初始状态
const state = {
  prefix: 'http://rap.taobao.org/mockjsdata/15673',
  userInfo: {
    openid: '123456'
  }
};

// 相关的 mutations
const mutations = {
  [SAVE_OPENID] (state, openid) {
    state.user.openid = openid;
  }
};

export default {
  state,
  mutations
};
